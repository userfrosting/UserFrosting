{
    "name": "userfrosting/sprinkle-core",
    "type": "sprinkle",
    "description": "Core module for UserFrosting.",
    "keywords": ["php user management", "usercake", "bootstrap"],
    "homepage": "https://github.com/userfrosting/UserFrosting",
    "license" : "MIT",
    "authors" : [
        {
            "name": "Alexander Weissman",
            "homepage": "https://alexanderweissman.com"
        }
    ],
    "version": "4.0.0",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/alexweissman/Slim-Csrf"
        },
        {
            "type": "vcs",
            "url": "https://github.com/userfrosting/i18n"
        }        
    ], 
    "require": {   
        "illuminate/database": "^5.2",
        "illuminate/events": "^5.2",
        "league/csv": "^8.1",        
        "monolog/monolog": "^1",
        "php": ">=5.5.9",
        "phpmailer/phpmailer": "5.2.10",  
        "rockettheme/toolbox": "1.3.1",
        "slim/slim": "^3",
        "slim/twig-view": "^1.2",
        "slim/csrf": "dev-feature-persistent-token",
        "symfony/http-foundation": "*",
        "twig/twig": "^1.18",
        "userfrosting/assets": "1.1",
        "userfrosting/config": "1.1",
        "userfrosting/fortress": "dev-master",
        "userfrosting/i18n": "1.0.x-dev",
        "userfrosting/session": "1.0",
        "userfrosting/support": "1.0",
        "vlucas/phpdotenv": "^2"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "UserFrosting\\Sprinkle\\Core\\": "src/"
        }
    }
}
